<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE validators PUBLIC "-//Apache Struts//XWork Validator 1.0.2//EN"
		"http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">

<validators>
	<field name="minBreak">
		<field-validator type="required">
			<message key="msg.validator.required"/>
		</field-validator>
		<field-validator type="int">
			<param name="min">0</param>	
			<param name="max">999</param>			
			<message key="msg.validator.intRange"/>
		</field-validator>
	</field>
	<field name="studentCount">
		<field-validator type="required">
			<message key="msg.validator.required"/>
		</field-validator>
		<field-validator type="int">
			<param name="min">0</param>				
			<message key="msg.validator.intRange"/>
		</field-validator>
	</field>
	<field name="fieldOfStudy">
		<field-validator type="required">
			<message key="msg.validator.required"/>
		</field-validator>
	</field>
	<field name="groupIdentifier">
		<field-validator type="requiredstring">
			<message key="msg.validator.required"/>
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">1</param>	
			<message key="msg.validator.stringMaxLength"/>
		</field-validator>
	</field>
	<field name="cohort.yearOfAdmission">
		<field-validator type="required">
			<message key="msg.validator.required"/>
		</field-validator>
		<field-validator type="int">
			<param name="min">1990</param>	
			<param name="max">2099</param>			
			<message key="msg.validator.intRange"/>
		</field-validator>
	</field>
</validators>